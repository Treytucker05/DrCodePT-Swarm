{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "scores": {
      "type": "object",
      "properties": {
        "reasoning": {
          "type": "integer"
        },
        "tool_usage": {
          "type": "integer"
        },
        "efficiency": {
          "type": "integer"
        },
        "overall": {
          "type": "integer"
        }
      }
    },
    "summary": {
      "type": "string"
    },
    "issues": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "recommendations": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  },
  "required": [
    "scores"
  ]
}