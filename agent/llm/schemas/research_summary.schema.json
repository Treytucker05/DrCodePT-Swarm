{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "summary_md": {
      "type": "string"
    },
    "key_findings": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "citations": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "url": {
            "type": "string"
          },
          "title": {
            "type": "string"
          }
        },
        "additionalProperties": false,
        "required": [
          "url",
          "title"
        ]
      }
    },
    "model_used": {
      "type": "string"
    }
  },
  "required": [
    "summary_md",
    "key_findings",
    "citations",
    "model_used"
  ],
  "additionalProperties": false
}