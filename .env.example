# ============================================
# DrCodePT-Swarm Agent - Configuration Template
# ============================================
# Copy this file to .env and fill in your values.
# NEVER commit the .env file - it contains secrets!

# =============================================================================
# LLM PROVIDERS (at least one required)
# =============================================================================

# OpenRouter - RECOMMENDED (cheap/free models available)
# Get your API key at: https://openrouter.ai/keys
OPENROUTER_API_KEY=sk-or-...

# OpenRouter model selection (optional, defaults shown)
# Examples: x-ai/grok-4.1-fast, anthropic/claude-3.5-sonnet, openai/gpt-4o-mini
OPENROUTER_MODEL=x-ai/grok-4.1-fast
# Per-task OpenRouter model overrides (optional)
# OPENROUTER_MODEL_PLANNER=x-ai/grok-4.1-fast
# OPENROUTER_MODEL_CHAT=x-ai/grok-4.1-fast
# OPENROUTER_MODEL_SUMMARIZE=x-ai/grok-4.1-fast
# OPENROUTER_MODEL_REASON=x-ai/grok-4.1-fast
# OPENROUTER_FALLBACK_PLANNER=nvidia/nemotron-nano-9b-v2:free
# OPENROUTER_FALLBACK_CHAT=nvidia/nemotron-nano-9b-v2:free
# OPENROUTER_FALLBACK_SUMMARIZE=nvidia/nemotron-nano-9b-v2:free
# OPENROUTER_FALLBACK_REASON=allenai/olmo-3.1-32b-think:free

# OpenAI - OPTIONAL (high quality but costs more)
# Get your API key at: https://platform.openai.com/api-keys
# OPENAI_API_KEY=sk-...
# OPENAI_MODEL=gpt-4o-mini

# Codex CLI - OPTIONAL (specialized for code, requires OAuth login)
# Path to codex executable (optional, searches PATH by default)
# CODEX_EXE_PATH=C:\path\to\codex.exe
# CODEX_CLI_PATH=C:\path\to\codex.exe
# CODEX_BIN=codex
# Codex model selection (optional)
# Fast model for simple tasks; reasoning model for harder tasks
CODEX_MODEL=gpt-5.1-codex-mini
CODEX_MODEL_FAST=gpt-5.1-codex-mini
CODEX_MODEL_REASON=gpt-5.2-codex
# Reasoning effort (low/medium/high)
CODEX_REASONING_EFFORT_FAST=low
CODEX_REASONING_EFFORT_REASON=high

# =============================================================================
# AGENT BEHAVIOR
# =============================================================================

# Primary LLM backend preference: codex | openrouter
TREYS_AGENT_LLM_PRIMARY=codex

# Maximum steps before agent stops (prevents runaway execution)
AUTO_MAX_STEPS=30

# Overall timeout in seconds
AUTO_TIMEOUT_SECONDS=600

# Planning mode: react (step-by-step) or plan_first (full plan upfront)
AUTO_PLANNER_MODE=react

# Enable web browser automation
AUTO_ENABLE_WEB_GUI=1

# Enable desktop UI automation
AUTO_ENABLE_DESKTOP=1

# Allow agent to ask user for clarification
AUTO_ALLOW_HUMAN_ASK=1

# Run pre-mortem analysis before execution
AUTO_PRE_MORTEM=0

# =============================================================================
# MEMORY
# =============================================================================
# Unified long-term memory DB path
# AGENT_MEMORY_DB=C:\path\to\autonomous_memory.sqlite3

# =============================================================================
# NOTEPAD DETERMINISTIC MODE
# =============================================================================
# file = write text to file then open Notepad with the file
AGENT_NOTEPAD_MODE=file
# AGENT_NOTEPAD_DIR=C:\Users\You\Desktop\Notes
# AGENT_NOTEPAD_FILE=C:\Users\You\Desktop\Notes\notepad_message.txt
# Verify UI result with screenshot + OCR (recommended)
AGENT_VERIFY_UI=1
# Strict verification: fail task if strong verification not possible
AGENT_VERIFY_STRICT=0
# AGENT_SCREENSHOT_DIR=C:\Users\You\Desktop\Agent_Screenshots

# =============================================================================
# AUTONOMY PROMPTS
# =============================================================================
# Auto-answer questions and auto-approve plans (DANGEROUS)
AGENT_AUTO_ANSWER=0
AGENT_AUTO_APPROVE=0

# =============================================================================
# SKILL REUSE
# =============================================================================
# Default similarity threshold for using a learned skill
SKILL_MATCH_THRESHOLD=0.7
# Lower threshold for local app automation (e.g., Notepad)
SKILL_MATCH_THRESHOLD_LOCAL=0.6

# ============================================================================= 
# SAFETY SETTINGS
# ============================================================================= 

# DANGEROUS: Set to 1 to disable all safety checks
# Only use for testing in isolated environments!
AGENT_UNSAFE_MODE=0

# File system access restrictions
# Set to 1 to allow access anywhere (DANGEROUS)
AUTO_FS_ANYWHERE=0

# Allowed file system roots (semicolon-separated)
# Agent can only access files under these paths
AUTO_FS_ALLOWED_ROOTS=C:\Users\treyt\Documents;C:\Users\treyt\Downloads

# Require approval for destructive actions
AGENT_APPROVAL_REQUIRED=1

# Kill switch file - if this file exists, agent stops immediately
# AGENT_KILL_SWITCH_FILE=C:\agent_stop.flag

# Domain allowlist for browser automation (comma-separated)
# AGENT_ALLOWED_DOMAINS=github.com,google.com,stackoverflow.com

# =============================================================================
# MEMORY & PERSISTENCE
# =============================================================================

# Path to memory database
# AGENT_MEMORY_PATH=agent/memory/agent_memory.db

# Enable memory persistence across sessions
AGENT_MEMORY_PERSIST=1

# =============================================================================
# LOGGING & TRACING
# =============================================================================

# Log level: DEBUG, INFO, WARNING, ERROR
AGENT_LOG_LEVEL=INFO

# Path to trace output directory
# AGENT_TRACE_DIR=runs/traces

# Enable structured JSON logging
AGENT_STRUCTURED_LOGS=1

# =============================================================================
# GOOGLE CALENDAR INTEGRATION (Optional)
# =============================================================================

# Path to OAuth credentials JSON file (client secret)
# Download from Google Cloud Console > APIs & Services > Credentials
GOOGLE_CALENDAR_CREDENTIALS=C:\Users\treyt\OneDrive\Desktop\DrCodePT-Swarm\agent\memory\google_credentials.json

# Optional: override token file path (tokens are stored in SecretStore by default)
# GOOGLE_CALENDAR_TOKEN=C:\Users\treyt\.drcodept_swarm\google_calendar\token.json

# Write token file even when SecretStore is available (0/1)
TREYS_AGENT_WRITE_GOOGLE_TOKEN_FILE=0

# Retry policy for Google APIs
GOOGLE_API_MAX_RETRIES=3
GOOGLE_API_RATE_LIMIT_WAIT_SECONDS=60

# =============================================================================
# DEVELOPMENT / TESTING
# =============================================================================

# Enable debug mode (verbose logging, no side effects)
# AGENT_DEBUG_MODE=0

# Run in eval mode (deterministic, record traces)
# AGENT_EVAL_MODE=0

# =============================================================================
# NOTES
# =============================================================================
# - Keep .env secure - it contains API keys!
# - Add .env to .gitignore
# - Restart agent after changing settings
# - See docs/ARCHITECTURE.md for more details
